{
  "name": "@pandino/bundle-installer-dom",
  "version": "0.8.1",
  "description": "Install Bundles defined in a browser's DOM",
  "module": "./dist/bundle-installer-dom.mjs",
  "types": "dist/bundle-installer-dom.d.mts",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/bundle-installer-dom.mjs"
    }
  },
  "private": true,
  "scripts": {
    "build": "rollup -c --environment=PRODUCTION",
    "build:dev": "rollup -c"
  },
  "keywords": [
    "pandino",
    "bundle",
    "installer",
    "dom"
  ],
  "author": "Norbert Herczeg <norbert.herczeg@blackbelt.hu>",
  "license": "EPL-2.0",
  "engines": {
    "node": ">=14.9.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@pandino/pandino-api": "^0.8.1",
    "@pandino/rollup-plugin-generate-manifest": "^0.8.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "rollup": "^2.70.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "pandino": {
    "manifest": {
      "Provide-Capability": "@pandino/bundle-installer;type=\"DOM\""
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
