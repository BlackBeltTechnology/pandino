{
  "name": "@pandino/pandino-event-admin",
  "version": "0.1.0",
  "description": "Reference implementation of the Pandino Event Admin API",
  "main": "./dist/cjs/pandino-event-admin.js",
  "module": "./dist/esm/pandino-event-admin.mjs",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/pandino-event-admin.js",
      "default": "./dist/esm/pandino-event-admin.mjs"
    }
  },
  "private": true,
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack"
  },
  "keywords": [],
  "author": "Norbert Herczeg <norbert.herczeg@blackbelt.hu>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@pandino/pandino-api": "^0.1.0",
    "@pandino/pandino-event-api": "^0.1.0",
    "@pandino/pandino-log-api": "^0.1.0",
    "handlebars-webpack-plugin": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": ["text", "json", "html"],
    "coverageThreshold": {
      "global": {
        "statements": 70,
        "branches": 65,
        "functions": 60,
        "lines": 70
      }
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testPathIgnorePatterns": [
      "src/activator.ts"
    ]
  }
}
